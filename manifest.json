{
  "manifest_version": 3,
  "name": "Hide 'Edit image' button from X (Twitter)",
  "version": "1.1",
  "description": "Hide 'Edit image' button from X (Twitter)",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://x.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
